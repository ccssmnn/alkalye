{
	"framework": "astro",
	"installCommand": "bun install",
	"buildCommand": "bun run build",
	"redirects": [
		{
			"source": "/doc/:match*",
			"destination": "/app/doc/:match*",
			"permanent": true
		},
		{
			"source": "/spaces/:match*",
			"destination": "/app/spaces/:match*",
			"permanent": true
		},
		{
			"source": "/settings",
			"destination": "/app/settings",
			"permanent": true
		},
		{
			"source": "/settings/:match*",
			"destination": "/app/settings/:match*",
			"permanent": true
		},
		{
			"source": "/privacy",
			"destination": "/app/privacy",
			"permanent": true
		},
		{
			"source": "/imprint",
			"destination": "/app/imprint",
			"permanent": true
		},
		{
			"source": "/welcome",
			"destination": "/app/welcome",
			"permanent": true
		},
		{
			"source": "/new",
			"destination": "/app/new",
			"permanent": true
		},
		{
			"source": "/local",
			"destination": "/app/local",
			"permanent": true
		},
		{
			"source": "/tutor/:slug",
			"destination": "/app/tutor/:slug",
			"permanent": true
		}
	],
	"rewrites": [
		{
			"source": "/app/:match*",
			"destination": "/app/index.html"
		}
	]
}
